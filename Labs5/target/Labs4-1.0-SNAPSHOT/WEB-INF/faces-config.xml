<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
        http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>/main.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>viewEnrolment</from-outcome>
            <to-view-id>/management/enrolments/view.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>addEnrolment</from-outcome>
            <to-view-id>/management/enrolments/add.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>removeEnrolment</from-outcome>
            <to-view-id>/management/enrolments/remove.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>addStudent</from-outcome>
            <to-view-id>/management/students/add.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>viewStudent</from-outcome>
            <to-view-id>/management/students/view.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>removeStudent</from-outcome>
            <to-view-id>/management/students/remove.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>addExam</from-outcome>
            <to-view-id>/management/exams/add.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>removeExam</from-outcome>
            <to-view-id>/management/exams/remove.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>viewExam</from-outcome>
            <to-view-id>/management/exams/view.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/students/add.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>


    <navigation-rule>
        <from-view-id>/management/exams/add.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/exams/remove.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/enrolments/remove.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/exams/add.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/students/view.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/students/remove.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/enrolments/view.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/enrolments/add.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/exams/view.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <managed-bean>
        <managed-bean-name>ExamBean</managed-bean-name>
        <managed-bean-class>com.jtechnologies.labs5.beans.ExamBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>StudentBean</managed-bean-name>
        <managed-bean-class>com.jtechnologies.labs5.beans.StudentBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>ExamInputBean</managed-bean-name>
        <managed-bean-class>com.jtechnologies.labs5.beans.ExamInputBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>StudentInputBean</managed-bean-name>
        <managed-bean-class>com.jtechnologies.labs5.beans.StudentInputBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>EnrolBean</managed-bean-name>
        <managed-bean-class>com.jtechnologies.labs5.beans.EnrolmentBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>SessionBean</managed-bean-name>
        <managed-bean-class>com.jtechnologies.labs5.beans.SessionBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <application>
        <resource-bundle>
            <base-name>Messages</base-name>
            <var>msg</var>
        </resource-bundle>
    </application>





</faces-config>