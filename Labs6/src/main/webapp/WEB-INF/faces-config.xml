        <managed-bean-class>com.jtechnologies.labs5.beans.SessionBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>UpdateStudentBean</managed-bean-name>
        <managed-bean-class>com.jtechnologies.labs5.beans.UpdateStudentBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>DeleteStudentBean</managed-bean-name>
        <managed-bean-class>com.jtechnologies.labs5.beans.DeleteStudentBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>SearchExamBean</managed-bean-name>
        <managed-bean-class>com.jtechnologies.labs5.beans.SearchExamBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>DeleteExamBean</managed-bean-name>
        <managed-bean-class>com.jtechnologies.labs5.beans.DeleteExamBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>DeleteEnrolmentBean</managed-bean-name>
        <managed-bean-class>com.jtechnologies.labs5.beans.DeleteEnrolmentBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>ResourceBean</managed-bean-name>
        <managed-bean-class>com.jtechnologies.labs5.beans.ResourceBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>ResourceBeanInput</managed-bean-name>
        <managed-bean-class>com.jtechnologies.labs5.beans.ResourceInputBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>DeleteResourceBean</managed-bean-name>
        <managed-bean-class>com.jtechnologies.labs5.beans.DeleteResourceBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>


    <application>
        <resource-bundle>
            <base-name>Messages</base-name>
            <var>msg</var>
        </resource-bundle>
    </application>





</faces-config>