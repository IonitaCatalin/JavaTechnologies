<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
        http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>/main.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>viewEnrolment</from-outcome>
            <to-view-id>/management/enrolments/view.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>addEnrolment</from-outcome>
            <to-view-id>/management/enrolments/add.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>deleteEnrolment</from-outcome>
            <to-view-id>/management/enrolments/delete.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>updateEnrolment</from-outcome>
            <to-view-id>/management/enrolments/update.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>addStudent</from-outcome>
            <to-view-id>/management/students/add.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>viewStudent</from-outcome>
            <to-view-id>/management/students/view.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>deleteStudent</from-outcome>
            <to-view-id>/management/students/delete.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>updateStudent</from-outcome>
            <to-view-id>/management/students/update.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>addExam</from-outcome>
            <to-view-id>/management/exams/add.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>deleteExam</from-outcome>
            <to-view-id>/management/exams/delete.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>viewExam</from-outcome>
            <to-view-id>/management/exams/view.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>updateExam</from-outcome>
            <to-view-id>/management/exams/update.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/students/add.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>


    <navigation-rule>
        <from-view-id>/management/exams/add.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/exams/delete.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>



    <navigation-rule>
        <from-view-id>/management/enrolments/delete.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/exams/add.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/students/view.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/students/delete.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/students/update.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/exams/update.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/enrolments/update.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/enrolments/view.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/enrolments/add.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/exams/search.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <navigation-rule>
        <from-view-id>/management/exams/view.xhtml</from-view-id>

        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>

    <managed-bean>
        <managed-bean-name>ExamBean</managed-bean-name>
        <managed-bean-class>com.jtechnologies.labs5.beans.ExamBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>StudentBean</managed-bean-name>
        <managed-bean-class>com.jtechnologies.labs5.beans.StudentBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>ExamInputBean</managed-bean-name>
        <managed-bean-class>com.jtechnologies.labs5.beans.ExamInputBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>StudentInputBean</managed-bean-name>
        <managed-bean-class>com.jtechnologies.labs5.beans.StudentInputBean</managed-bean-class>
